{
  "compilerOptions": {
    "module": "CommonJS",
    "target": "ES2021",
    "strict": true,
    "strictNullChecks": false,
    "removeComments": true,
    "noEmitOnError": true,
    "noFallthroughCasesInSwitch": true,
    "noUnusedLocals": true,
    "newLine": "lf",
    "noImplicitAny": true,
    "noUnusedParameters": true,
    "esModuleInterop": true,
    "importHelpers": true,
    "forceConsistentCasingInFileNames": true,
    "outDir": "script/js/"
  }
}
